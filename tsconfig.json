{
  "compilerOptions": {
      "baseUrl": "./src",
      "target": "es5",
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "paths": {
        "@components/*": ["components/*"],
        "@customTypes/*": ["types/*"],
        "@hooks/*": ["hooks/*"],
        "@pages/*": ["pages/*"],
        "@redux/*": ["redux/*"],
        "@styles/*": ["styles/*"],
        "@utils/*": ["utils/*"]
      },
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,
      "jsx": "react-jsx"
  },
  "include": [
      "src"
  ]
}